{
  "name": "tecton-interview",
  "version": "1.0.0",
  "main": "www",
  "license": "MIT",
  "scripts": {
    "build": "yarn run clean && yarn run build:prod",
    "build:prod": "NODE_ENV=production webpack --config ./client/webpack.config.js --bail",
    "build:dev": "NODE_ENV=development webpack --progress --colors --config ./client/webpack.config.js",
    "clean": "rm -rf ./public && rm -rf ./node_modules/.cache/babel-loader",
    "format": "prettier --config ./.prettierrc --write 'client/**/*.js' 'client/**/*.jsx' 'server/**/*.js'",
    "lint": "eslint --fix .",
    "lint:ejs": "ejslint ./views/main.ejs && ejslint ./views/error.ejs && ejslint ./views/partials/_body.ejs && ejslint ./views/partials/_head.ejs && ejslint ./views/partials/_scripts.ejs && ejslint ./views/partials/_styles.ejs",
    "upload": "./scripts/s3_upload --directory ./public --bucket justforwhatever",
    "start:dev": "PORT=8008 NODE_ENV=development CONFIG=development nodemon -e js,json,jsx ./www | bunyan -o short"
  },
  "dependencies": {
    "@babel/register": "7.8.6",
    "@loadable/component": "5.12.0",
    "axios": "0.19.2",
    "bootstrap": "4.4.1",
    "bunyan": "1.8.12",
    "classnames": "2.2.6",
    "compression": "1.7.4",
    "ejs": "3.0.1",
    "express": "4.17.1",
    "helmet": "3.21.3",
    "lodash": "4.17.21",
    "moment": "2.24.0",
    "nconf": "0.10.0",
    "pretty-hrtime": "1.0.3",
    "prop-types": "15.7.2",
    "qs": "6.9.1",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "react-transition-group": "4.3.0",
    "react-window": "1.8.5",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-react": "7.8.3",
    "@babel/runtime": "7.8.7",
    "aws-sdk": "2.652.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.0.6",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "css-loader": "3.4.2",
    "ejs-lint": "1.0.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "file-loader": "5.1.0",
    "meow": "6.1.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.1",
    "nodemon": "2.0.2",
    "prettier": "1.19.1",
    "react-dev-utils": "10.2.0",
    "sass-loader": "8.0.2",
    "terser-webpack-plugin": "2.3.5",
    "url-loader": "3.0.0",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-middleware": "3.7.2",
    "webpack-manifest-plugin": "2.2.0"
  }
}
